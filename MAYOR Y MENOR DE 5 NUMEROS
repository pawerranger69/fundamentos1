#include <iostream>
using namespace std;
int main()
{
    int num1,num2,num3,num4,num5,mayor,menor;
    cout<<"Ingresar primer numero: ";
    cin>>num1;
    mayor=num1;
    menor=num1;
    cout<<"Ingresar segundo numero: ";
    cin>>num2;
    if(num2>mayor) 
       mayor=num2;
    if(num2<menor) 
       menor=num2;
    cout<<"Ingresar el tercer numero: ";
    cin>>num3;
    if(num3>mayor)
       mayor=num3;
    if(num3<menor)
       menor=num3;
    cout<<"Ingresar el cuarto numero: ";
    cin>>num4;
    if(num4>mayor)
       mayor=num4;
    if(num4<menor)
       menor=num4;
    cout<<"Ingresar el quinto numero: ";
    cin>>num5;
    if(num5>mayor)
       mayor=num5;
    if(num5<menor)
       menor=num5;
    cout<<"El numero mayor es: "<<mayor<<" y el numero menor es: "<<menor;
    return 0;
}
